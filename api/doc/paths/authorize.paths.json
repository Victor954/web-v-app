{
    "/authorize/login": {
        "post": {
            "tags": [
                "authorize"
            ],
            "summary": "Войти",
            "requestBody": {
                "description": "Схема для входа",
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/Login"
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "Токены для входа",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Tokens"
                            }
                        }
                    }
                },
                "400": {
                    "description": "Обработанная ошибка",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ServerError"
                            }
                        }
                    }
                },
                "500": {
                    "description": "Необработанная ошибка",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ServerError"
                            }
                        }
                    }
                }
            }
        }
    }
}